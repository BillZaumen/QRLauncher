<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="application/vnd.bzdev.qrlauncher-config">
    <comment>QRLauncher configuration</comment>
    <magic priority="80">
      <match type="string" offset="0"
	     value="#(!M.T application/vnd.bzdev.qrlauncher-config)"/>
      <!-- match when a byte order mark is present -->
      <match type="big32" value="0xefbbbf3c" offset="0">
	<match type="string" offset="4"
	       value="#(!M.T application/vnd.bzdev.qrlauncher-config)"/>
      </match>
    </magic>
    <glob pattern="*.qrl"/>
  </mime-type>
</mime-info>
